<ul>
  <%= content_tag("li", (link_to "Detect metadata source", "/repos/#{@object.id}/detect_metadata", :id => @object.id, method: :post)) %>
  <%= render_sources_table(@object) unless @object.metadata_sources.nil? %>
  <% @mappings = _build_form_list(@object) -%>
  <%= render :partial => "rails_admin/main/forms/map_metadata", :locals => {:mappings_sets => @mappings}%>
</ul>
